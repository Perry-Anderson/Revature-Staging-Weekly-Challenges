package com.revature;

public class Launcher {
	public static void main(String[] args) {
		System.out.println(sherlock(3,9));
		System.out.println(sherlock(17,24));
		System.out.println(sherlock(1,1000000000));
	}
	
	public static int sherlock(int a, int b) {
		 double start = Math.ceil(Math.sqrt(a));
	     double end = Math.floor(Math.sqrt(b));
	     return  (int)(end-start+1);
	}
	
}
